
/*---------æ ‘----------*/
.ztree li span.button.ico_docu{
     background-position: -127px -64px;
     margin-right: 2px;
     vertical-align: top;
}
.tree-menu{
    width: 198px;
    border: 1px solid #d6e5f5;
    background: #fff;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
}
.tree-menu .title{
    line-height: 24px;
    background: #f4f7fb;
    text-indent: 9px;
    font-size: 14px;
}
.search-bar{
    overflow: hidden;
}
#keyword,#search-bt{
    display: block;
    float: left;
    outline: none;
}
#keyword{
    border: none;
    border: 1px solid #d6e5f5;
    line-height: 25px;
    width: 164px;
    text-indent: 9px;
}
#search-bt{
    width: 32px;
    height: 30px;
    border: 0;
    margin: 0;
    padding: 0;
    background: url(../img/search.png) no-repeat;
}
#search-bt:hover{
    cursor:pointer;
}

[v-cloak] { display: none }

.nav-pills{
    background: #e0e8f3;
}
.nav>li>a {
    padding: 10px 25px;
    color: #333;
}
.nav-pills>li.active>a, .nav-pills>li.active>a:focus, .nav-pills>li.active>a:hover {
    color: #fff;
    background-color: #428cf7;
}

.time-query{
    background: #f4f7fc;
    padding: 20px 15px;
    margin-top: 10px;
    z-index: 9999;
}
.btn-primary {
    color: #fff;
    background-color: #428cf7;
    border-color: #428cf7;
    margin-left: 15px;
}
.calendar{
    position: relative;
}

.sign{
    width: 6px;
    height: 24px;
    background: #3f8cf6;
    display: block;
    position: absolute;
    left: 0;
    top: 43px;
    z-index: 1;
}

.isNone{
    display: none !important;
}