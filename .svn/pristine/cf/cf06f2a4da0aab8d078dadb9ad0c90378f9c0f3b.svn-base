<!DOCTYPE html>
<html>
<head>
    <title>系统代码表</title>
		<#include "/header.html">
</head>
<body>
<div id="rrapp" v-cloak>
    <div v-show="showList">
        <div class="grid-btn">
			<#if shiro.hasPermission("dict:dmdict:save")>
            <a class="btn btn-primary" @click="add"><i class="fa fa-plus"></i>&nbsp;新增</a>
			</#if>
			<#if shiro.hasPermission("dict:dmdict:update")>
            <a class="btn btn-primary" @click="update"><i class="fa fa-pencil-square-o"></i>&nbsp;修改</a>
			</#if>
			<#if shiro.hasPermission("dict:dmdict:delete")>
            <a class="btn btn-primary" @click="del"><i class="fa fa-trash-o"></i>&nbsp;删除</a>
			</#if>
        </div>
        <table id="jqGrid"></table>
        <div id="jqGridPager"></div>
    </div>

    <div v-show="!showList" class="panel panel-default">
        <div class="panel-heading">{{title}}</div>
        <form class="form-horizontal">
							                    <div class="form-group">
                        <div class="col-sm-2 control-label">类型ID</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="dmDict.lxid" placeholder="请输入类型ID"/>
                        </div>
                    </div>
																		                    <div class="form-group">
                        <div class="col-sm-2 control-label">字段值</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="dmDict.zdcode" placeholder="请输入字段值"/>
                        </div>
                    </div>
					<div class="form-group">
                        <div class="col-sm-2 control-label">父ID</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="dmDict.parentid" placeholder="请输入父ID"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">字段名称</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="dmDict.zdmc" placeholder="请输入字段名称"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">属性 </div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="dmDict.property" placeholder="请输入属性"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">选用标志</div>
                        <div class="col-sm-10">
                            <!-- <input type="text" class="form-control" v-model="dmDict.xybz" placeholder="请输入选用标志"/> -->
                            <select class="form-control" v-model="dmDict.xybz">
                            	<option value="Y">选用</option>
                            	<option value="N">禁用</option>
                            </select>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">排序</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="dmDict.zdorder" placeholder="请输入排序"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">备注</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="dmDict.bz" placeholder="请输入备注"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">英文名称</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="dmDict.en" placeholder="请输入英文名称"/>
                        </div>
                    </div>
							            <div class="form-group">
                <div class="col-sm-2 control-label"></div>
                <input type="button" class="btn btn-primary" @click="saveOrUpdate" value="确定"/>
                &nbsp;&nbsp;<input type="button" class="btn btn-warning" @click="reload" value="返回"/>
            </div>
        </form>
    </div>
</div>

<script src="${request.contextPath}/statics/js/modules/dict/dmdict.js?_${.now?long}"></script>
</body>
</html>