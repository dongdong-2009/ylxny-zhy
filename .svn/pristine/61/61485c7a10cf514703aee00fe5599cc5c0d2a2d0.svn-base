
/*报警信息 报警分析*/
ul,li{text-decoration: none !important;}
.nav-pills{
    background: #e0e8f3;
}
.nav>li>a {
    padding: 10px 25px;
    color: #333;
}
.nav-pills>li.active>a, .nav-pills>li.active>a:focus, .nav-pills>li.active>a:hover {
    color: #fff;
    background-color: #428cf7;
}

.time-query{
    background: #f4f7fc;
    padding: 0 15px 20px 15px;
    margin-top: 10px;
}
.btn-primary {
    color: #fff;
    background-color: #428cf7;
    border-color: #428cf7;
    margin-left: 15px;
}
.calendar{
    position: relative;
}

.sign{
    width: 6px;
    height: 24px;
    background: #3f8cf6;
    display: block;
    position: absolute;
    left: 0;
    top: 33px;
    z-index: 1;
}

.facility{
    line-height: 58px;
}
.facility span{
    padding-left: 9px;
}

.seltime{
    margin-top: 10px;
    background: #f4f7fc;
}
.selected-time{
    margin-bottom: 0;
    padding-left: 0;
}
.selected-time li{
    height: 46px;
    line-height: 46px;
    display: inline;
    padding-right: 25px;
}
.selected-time span{
    top: 2px;
    margin-right: 5px;
}
.selected-time span:hover{
    cursor: pointer;
}