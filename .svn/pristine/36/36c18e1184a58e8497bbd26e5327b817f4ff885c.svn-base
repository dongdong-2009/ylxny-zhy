
/*能效分析 能耗对比*/
.nav-pills{
    background: #e0e8f3;
}
.nav>li>a {
    padding: 10px 25px;
    color: #333;
}
.nav-pills>li.active>a, .nav-pills>li.active>a:focus, .nav-pills>li.active>a:hover {
    color: #fff;
    background-color: #428cf7;
}

.time-query{
    background: #f4f7fc;
    padding: 20px 15px;
    margin-top: 10px;
}
.btn-primary {
    color: #fff;
    background-color: #428cf7;
    border-color: #428cf7;
    margin-left: 15px;
}
.calendar{
    position: relative;
}

.sign{
    width: 6px;
    height: 24px;
    background: #3f8cf6;
    display: block;
    position: absolute;
    left: 0;
    top: 33px;
    z-index: 1;
}